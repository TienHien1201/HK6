ype.toLowerCase() === 'tea');\r\n\r\n      // T√≠nh to√°n ph·∫°m vi d·ªØ li·ªáu hi·ªÉn th·ªã d·ª±a tr√™n ph√¢n trang\r\n      const start = (this.currentpage - 1) * this.limit;\r\n      const end = start + this.limit;\r\n\r\n      // Tr·∫£ v·ªÅ danh s√°ch s·∫£n ph·∫©m sau khi ph√¢n trang\r\n      return teaProducts.slice(start, end);\r\n    },\r\n\r\n    fillterproductNews() {\r\n      const start = (this.currentpage - 1) * this.limit;\r\n      const end = start + this.limit;\r\n      return this.newsItems.slice(start, end);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.startSlideshow();\r\n    //  this.$store.dispatch('fetchProducts');\r\n  },\r\n  methods: {\r\n    changpage(page) {\r\n      if (page >= 1 && page <= this.toltalpage) {\r\n        this.currentpage = page;\r\n      }\r\n    },\r\n    startSlideshow() {\r\n      setInterval(() => {\r\n        this.currentImageIndex = (this.currentImageIndex + 1) % this.slideshowImages.length;\r\n      }, 3000);\r\n    },\r\n    loadMoreStores() {\r\n      if (this.showMore) {\r\n        // N·∫øu ƒëang hi·ªÉn th·ªã t·∫•t c·∫£, ·∫©n c√°c c·ª≠a h√†ng m·ªõi th√™m v√† quay l·∫°i danh s√°ch ban ƒë·∫ßu\r\n        this.displayedStores = this.storeList.slice(0, 2);\r\n        this.showMore = false; // Quay v·ªÅ tr·∫°ng th√°i n√∫t \"Xem th√™m\"\r\n      } else {\r\n        // N·∫øu nh·∫•n \"Xem th√™m\", th√™m d·ªØ li·ªáu m·ªõi v√†o danh s√°ch v√† hi·ªÉn th·ªã to√†n b·ªô\r\n        const moreStores = [\r\n          {\r\n            id: 4,\r\n            name: 'HCM-CH Bitexco',\r\n            address: 'T√≤a nh√† Bitexco, Qu·∫≠n 1, TP. H·ªì Ch√≠ Minh',\r\n            phone: '(028) 3821 8888',\r\n            hours: '09:00 - 22:00',\r\n            status: 'M·ªü c·ª≠a',\r\n          },\r\n          {\r\n            id: 5,\r\n            name: 'HCM-CH Landmark 81',\r\n            address: 'Landmark 81, Qu·∫≠n B√¨nh Th·∫°nh, TP. H·ªì Ch√≠ Minh',\r\n            phone: '(028) 3775 1888',\r\n            hours: '09:00 - 22:00',\r\n       