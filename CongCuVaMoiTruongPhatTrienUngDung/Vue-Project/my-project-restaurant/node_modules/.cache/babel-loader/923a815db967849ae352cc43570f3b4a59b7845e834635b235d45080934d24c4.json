{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport eventBus from '@/evenBus';\nexport default {\n  name: 'ComSignIn',\n  data() {\n    return {\n      username: '',\n      //an\n      password: '',\n      //1234\n      error: ''\n    };\n  },\n  methods: {\n    async SignIn() {\n      const response = await axios.get('http://localhost:3000/user');\n      const users = response.data;\n      const trimmedUsername = this.username.trim();\n      const trimmedPassword = this.password.trim();\n      const user = users.find(x => x.username === trimmedUsername && x.password === trimmedPassword);\n      if (user) {\n        // L∆∞u th√¥ng tin v√†o local\n        localStorage.setItem('userCurrent', JSON.stringify(user));\n        eventBus.emit('loginSuccess', user);\n        this.$router.push('/');\n      } else {\n        this.error = 'T√™n ƒëƒÉng nh·∫≠p, m·∫≠t kh·∫©u kh√¥ng ƒë√∫ng';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","eventBus","name","data","username","password","error","methods","SignIn","response","get","users","trimmedUsername","trim","trimmedPassword","user","find","x","localStorage","setItem","JSON","stringify","emit","$router","push"],"sources":["D:\\BaiHoc\\HK6\\CongCuVaMoiTruongPhatTrienUngDung\\Vue-Project\\my-project-restaurant\\src\\components\\ComSignIn.vue"],"sourcesContent":["<template><div class=\"product-coffe-list\">\r\n  <div class=\"product-grid\">\r\n    <Product v-for=\"item in filteredProductCoffe\" :key=\"item.id\" :product=\"item\" />\r\n  </div>\r\n  <!-- Pagination for Coffee -->\r\n  <div class=\"pagination\">\r\n    <button @click=\"changePage('coffe', currentPageCoffe - 1)\" :disabled=\"currentPageCoffe === 1\">\r\n      Trang tr∆∞·ªõc\r\n    </button>\r\n    <button\r\n      v-for=\"page in totalPageCoffe\"\r\n      :key=\"page\"\r\n      @click=\"changePage('coffe', page)\"\r\n      :class=\"{ active: page === currentPageCoffe }\"\r\n    >\r\n      {{ page }}\r\n    </button>\r\n    <button\r\n      @click=\"changePage('coffe', currentPageCoffe + 1)\"\r\n      :disabled=\"currentPageCoffe === totalPageCoffe\"\r\n    >\r\n      Trang sau\r\n    </button>\r\n  </div>\r\n</div>\r\n\r\n<h2>TH·ª∞C ƒê∆†N M√ìN KH√ÅC (FOOD MENU)</h2>\r\n<div class=\"product-tea-list\">\r\n  <div class=\"product-grid\">\r\n    <Product v-for=\"item in filteredProductTea\" :key=\"item.id\" :product=\"item\" />\r\n  </div>\r\n  <!-- Pagination for Tea -->\r\n  <div class=\"pagination\">\r\n    <button @click=\"changePage('tea', currentPageTea - 1)\" :disabled=\"currentPageTea === 1\">\r\n      Trang tr∆∞·ªõc\r\n    </button>\r\n    <button\r\n      v-for=\"page in totalPageTea\"\r\n      :key=\"page\"\r\n      @click=\"changePage('tea', page)\"\r\n      :class=\"{ active: page === currentPageTea }\"\r\n    >\r\n      {{ page }}\r\n    </button>\r\n    <button @click=\"changePage('tea', currentPageTea + 1)\" :disabled=\"currentPageTea === totalPageTea\">\r\n      Trang sau\r\n    </button>\r\n  </div>\r\n</div>\r\n\r\n<h2>üì∞ Tin T·ª©c</h2>\r\n<div class=\"news-list\">\r\n  <div class=\"news-item\" v-for=\"news in filteredProductNews\" :key=\"news.id\">\r\n    <img :src=\"news.image\" alt=\"news image\" class=\"news-image\" />\r\n    <h3>{{ news.title }}</h3>\r\n    <p>{{ news.description }}</p>\r\n    <button class=\"read-more\">ƒê·ªçc Ti·∫øp</button>\r\n  </div>\r\n  <!-- Pagination for News -->\r\n  <div class=\"pagination\">\r\n    <button @click=\"changePage('news', currentPageNews - 1)\" :disabled=\"currentPageNews === 1\">\r\n      Trang tr∆∞·ªõc\r\n    </button>\r\n    <button\r\n      v-for=\"page in totalPageNews\"\r\n      :key=\"page\"\r\n      @click=\"changePage('news', page)\"\r\n      :class=\"{ active: page === currentPageNews }\"\r\n    >\r\n      {{ page }}\r\n    </button>\r\n    <button @click=\"changePage('news', currentPageNews + 1)\" :disabled=\"currentPageNews === totalPageNews\">\r\n      Trang sau\r\n    </button>\r\n  </div>\r\n</div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport eventBus from '@/evenBus';\r\nexport default {\r\n  name: 'ComSignIn',\r\n  data() {\r\n    return {\r\n      username: '', //an\r\n      password: '', //1234\r\n      error: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async SignIn() {\r\n      const response = await axios.get('http://localhost:3000/user');\r\n      const users = response.data;\r\n\r\n      const trimmedUsername = this.username.trim();\r\n      const trimmedPassword = this.password.trim();\r\n\r\n      const user = users.find(\r\n        (x) => x.username === trimmedUsername && x.password === trimmedPassword\r\n      );\r\n      if (user) {\r\n        // L∆∞u th√¥ng tin v√†o local\r\n        localStorage.setItem('userCurrent', JSON.stringify(user));\r\n        eventBus.emit('loginSuccess', user);\r\n        this.$router.push('/');\r\n      } else {\r\n        this.error = 'T√™n ƒëƒÉng nh·∫≠p, m·∫≠t kh·∫©u kh√¥ng ƒë√∫ng';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.background-img {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n  z-index: -1;\r\n}\r\n\r\n.login-form {\r\n  background-color: #c6a464;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  width: 300px;\r\n  text-align: center;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.login-form h2 {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.login-form input {\r\n  width: 100%;\r\n  padding: 10px;\r\n  margin: 10px 0;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n}\r\n\r\n.login-form button {\r\n  width: 100%;\r\n  padding: 10px;\r\n  margin: 10px 0;\r\n  background-color: #ff5722;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.login-options {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  font-size: 0.9em;\r\n  color: #555;\r\n}\r\n\r\n.social-login button {\r\n  width: 45%;\r\n  padding: 10px;\r\n  margin-top: 10px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.social-login button:first-child {\r\n  background-color: #3b5998; /* Facebook color */\r\n  color: white;\r\n}\r\n\r\n.social-login button:last-child {\r\n  background-color: #db4437; /* Google color */\r\n  color: white;\r\n}\r\n\r\n.login-form p {\r\n  margin-top: 20px;\r\n  font-size: 0.9em;\r\n}\r\n\r\n.login-form p a {\r\n  color: #ff5722;\r\n  text-decoration: none;\r\n}\r\n</style>\r\n"],"mappings":";AAgFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,WAAW;AAChC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MAAE;MACdC,QAAQ,EAAE,EAAE;MAAE;MACdC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAG;MACb,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,4BAA4B,CAAC;MAC9D,MAAMC,KAAI,GAAIF,QAAQ,CAACN,IAAI;MAE3B,MAAMS,eAAc,GAAI,IAAI,CAACR,QAAQ,CAACS,IAAI,CAAC,CAAC;MAC5C,MAAMC,eAAc,GAAI,IAAI,CAACT,QAAQ,CAACQ,IAAI,CAAC,CAAC;MAE5C,MAAME,IAAG,GAAIJ,KAAK,CAACK,IAAI,CACpBC,CAAC,IAAKA,CAAC,CAACb,QAAO,KAAMQ,eAAc,IAAKK,CAAC,CAACZ,QAAO,KAAMS,eAC1D,CAAC;MACD,IAAIC,IAAI,EAAE;QACR;QACAG,YAAY,CAACC,OAAO,CAAC,aAAa,EAAEC,IAAI,CAACC,SAAS,CAACN,IAAI,CAAC,CAAC;QACzDd,QAAQ,CAACqB,IAAI,CAAC,cAAc,EAAEP,IAAI,CAAC;QACnC,IAAI,CAACQ,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB,OAAO;QACL,IAAI,CAAClB,KAAI,GAAI,oCAAoC;MACnD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}