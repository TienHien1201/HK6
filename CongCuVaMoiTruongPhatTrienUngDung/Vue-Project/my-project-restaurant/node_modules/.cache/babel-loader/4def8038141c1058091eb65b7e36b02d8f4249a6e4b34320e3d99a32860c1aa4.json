{"ast":null,"code":"import Product from './ComProductCoffe.vue';\nimport itemCoffes from '../data/itemsCoffe';\nimport itemTeas from '../data/itemsTea';\nimport TinTucItems from '@/data/TinTuc';\nimport storeList from '@/data/storeList';\nimport { Caegories } from '../data/categories';\nexport default {\n  name: 'ComHome',\n  components: {\n    Product\n  },\n  data() {\n    return {\n      showMore: false,\n      // Tr·∫°ng th√°i c·ªßa n√∫t\n      currentpage: 1,\n      limit: 4,\n      products: itemCoffes,\n      productTea: itemTeas,\n      categories: Caegories,\n      slideshowImages: [require('@/assets/IntroHomeImg.jpg'), require('@/assets/IntroHomeImg2.png')],\n      currentImageIndex: 0,\n      newsItems: TinTucItems,\n      storeList: storeList\n    };\n  },\n  computed: {\n    toltalpage() {\n      return Math.ceil(this.products.length / this.limit);\n    },\n    fillterproductCoffe() {\n      const start = (this.currentpage - 1) * this.limit;\n      const end = start + this.limit;\n      return this.products.slice(start, end);\n    },\n    fillterproductTeas() {\n      const start = (this.currentpage - 1) * this.limit;\n      const end = start + this.limit;\n      return this.productTea.slice(start, end);\n    },\n    fillterproductNews() {\n      const start = (this.currentpage - 1) * this.limit;\n      const end = start + this.limit;\n      return this.newsItems.slice(start, end);\n    }\n  },\n  mounted() {\n    this.startSlideshow();\n  },\n  methods: {\n    changpage(page) {\n      if (page >= 1 && page <= this.toltalpage) {\n        this.currentpage = page;\n      }\n    },\n    startSlideshow() {\n      setInterval(() => {\n        this.currentImageIndex = (this.currentImageIndex + 1) % this.slideshowImages.length;\n      }, 3000);\n    },\n    loadMoreStores() {\n      if (this.showMore) {\n        // N·∫øu ƒëang hi·ªÉn th·ªã t·∫•t c·∫£, ·∫©n c√°c c·ª≠a h√†ng m·ªõi th√™m v√† quay l·∫°i danh s√°ch ban ƒë·∫ßu\n        this.displayedStores = this.storeList.slice(0, 2);\n        this.showMore = false; // Quay v·ªÅ tr·∫°ng th√°i n√∫t \"Xem th√™m\"\n      } else {\n        // N·∫øu nh·∫•n \"Xem th√™m\", th√™m d·ªØ li·ªáu m·ªõi v√†o danh s√°ch v√† hi·ªÉn th·ªã to√†n b·ªô\n        const moreStores = [{\n          id: 4,\n          name: 'HCM-CH Bitexco',\n          address: 'T√≤a nh√† Bitexco, Qu·∫≠n 1, TP. H·ªì Ch√≠ Minh',\n          phone: '(028) 3821 8888',\n          hours: '09:00 - 22:00',\n          status: 'M·ªü c·ª≠a'\n        }, {\n          id: 5,\n          name: 'HCM-CH Landmark 81',\n          address: 'Landmark 81, Qu·∫≠n B√¨nh Th·∫°nh, TP. H·ªì Ch√≠ Minh',\n          phone: '(028) 3775 1888',\n          hours: '09:00 - 22:00',\n          status: 'M·ªü c·ª≠a'\n        }];\n\n        // C·∫≠p nh·∫≠t danh s√°ch `storeList` v·ªõi c√°c m·ª•c m·ªõi\n        this.storeList = [...this.storeList, ...moreStores];\n        this.displayedStores = this.storeList; // Hi·ªÉn th·ªã to√†n b·ªô danh s√°ch\n        this.showMore = true; // ƒê·ªïi tr·∫°ng th√°i n√∫t th√†nh \"^\"\n      }\n    }\n  }\n};","map":{"version":3,"names":["Product","itemCoffes","itemTeas","TinTucItems","storeList","Caegories","name","components","data","showMore","currentpage","limit","products","productTea","categories","slideshowImages","require","currentImageIndex","newsItems","computed","toltalpage","Math","ceil","length","fillterproductCoffe","start","end","slice","fillterproductTeas","fillterproductNews","mounted","startSlideshow","methods","changpage","page","setInterval","loadMoreStores","displayedStores","moreStores","id","address","phone","hours","status"],"sources":["D:\\BaiHoc\\HK6\\CongCuVaMoiTruongPhatTrienUngDung\\Vue-Project\\my-project-restaurant\\src\\components\\ComHome.vue"],"sourcesContent":["<template>\r\n  <div class=\"container-home\">\r\n    <!-- Automatic Slideshow -->\r\n    <div class=\"slideshow-container\">\r\n      <img :src=\"slideshowImages[currentImageIndex]\" class=\"slideshow-image\" />\r\n    </div>\r\n    