   totalPageTea() {\r\n      return Math.ceil(this.teaProducts.length / this.limit); // Paginate based on teaProducts\r\n    },\r\n    totalPageNews() {\r\n      return Math.ceil(this.newsItems.length / this.limit);\r\n    },\r\n    filteredProductCoffe() {\r\n      const start = (this.currentPageCoffe - 1) * this.limit;\r\n      const end = start + this.limit;\r\n      return this.products\r\n        .filter((product) => product.type.toLowerCase() === 'coffe')\r\n        .slice(start, end);\r\n    },\r\n    filteredProductTea() {\r\n      const start = (this.currentPageTea - 1) * this.limit;\r\n      const end = start + this.limit;\r\n      return this.teaProducts.slice(start, end); // Paginate the tea products\r\n    },\r\n    filteredProductNews() {\r\n      const start = (this.currentPageNews - 1) * this.limit;\r\n      const end = start + this.limit;\r\n      return this.newsItems.slice(start, end);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.startSlideshow();\r\n    this.fetchTeaProducts(); // Fetch tea products when the component is mounted\r\n  },\r\n  methods: {\r\n    // Fetch tea products from the API\r\n    async fetchTeaProducts() {\r\n      try {\r\n        const fetchedProducts = await fetchProducts(); // Fetch all products from API\r\n        this.teaProducts = fetchedProducts.filter((item) => item.type === 'Tea'); // Filter out tea products\r\n      } catch (error) {\r\n        console.error(\"Error fetching tea products:\", error);\r\n      }\r\n    },\r\n    // Change page for each section (coffee, tea, and news)\r\n    changePage(type, page) {\r\n      if (type === 'coffe' && page >= 1 && page <= this.totalPageCoffe) {\r\n        this.currentPageCoffe = page;\r\n      } else if (type === 'tea' && page >= 1 && page <= this.totalPageTea) {\r\n        this.currentPageTea = page; // Update tea page\r\n      } else if (type === 'news' && page >= 1 && page <= this.totalPageNews) {\r\n        this.currentPageNews = page;\r\n      }\r\n    },\r\n    s