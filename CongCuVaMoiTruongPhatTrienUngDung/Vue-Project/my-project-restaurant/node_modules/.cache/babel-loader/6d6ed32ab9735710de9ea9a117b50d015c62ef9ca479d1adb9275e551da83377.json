{"ast":null,"code":"import TinTucItems from '@/data/TinTuc';\nimport storeList from '@/data/storeList';\nimport ComCoffeeVue from './ComCoffee.vue';\nimport ComTeaVue from './ComTea.vue';\nexport default {\n  name: 'ComHome',\n  components: {\n    ComCoffeeVue,\n    ComTeaVue\n  },\n  data() {\n    return {\n      showMore: false,\n      // Tr·∫°ng th√°i c·ªßa n√∫t\n      currentPageCoffe: 1,\n      currentPageTea: 1,\n      currentPageNews: 1,\n      limit: 4,\n      slideshowImages: [require('@/assets/IntroHomeImg.jpg'), require('@/assets/IntroHomeImg2.png')],\n      currentImageIndex: 0,\n      newsItems: TinTucItems,\n      storeList: storeList,\n      teaProducts: [] // Store the tea products\n    };\n  },\n  computed: {\n    totalPageCoffe() {\n      return Math.ceil(this.products.filter(product => product.type.toLowerCase() === 'coffe').length / this.limit);\n    },\n    totalPageTea() {\n      return Math.ceil(this.teaProducts.length / this.limit); // Paginate based on teaProducts\n    },\n    totalPageNews() {\n      return Math.ceil(this.newsItems.length / this.limit);\n    },\n    filteredProductCoffe() {\n      const start = (this.currentPageCoffe - 1) * this.limit;\n      const end = start + this.limit;\n      return this.products.filter(product => product.type.toLowerCase() === 'coffe').slice(start, end);\n    },\n    filteredProductTea() {\n      const start = (this.currentPageTea - 1) * this.limit;\n      const end = start + this.limit;\n      return this.teaProducts.slice(start, end); // Paginate the tea products\n    },\n    filteredProductNews() {\n      const start = (this.currentPageNews - 1) * this.limit;\n      const end = start + this.limit;\n      return this.newsItems.slice(start, end);\n    }\n  },\n  mounted() {\n    this.startSlideshow();\n    this.fetchTeaProducts(); // Fetch tea products when the component is mounted\n  },\n  methods: {\n    // Fetch tea products from the API\n    async fetchTeaProducts() {\n      try {\n        const 